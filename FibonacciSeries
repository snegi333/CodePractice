public class FibonacciSeries {

    public static void main(String[] args) {
        int count = 0, x = 0, y = 1;
        System.out.print(x + " " + y);
        while (count < 20) {
            int sum = x + y;
            x = y;
            y = sum;
            System.out.print(" ");
            System.out.print(sum);
            count++;
        }
    }
}
